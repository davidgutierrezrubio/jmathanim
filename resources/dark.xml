<JMathAnimConfig>
     <background>
        <color>#192841</color>
    </background>
    <templates>
        <template name="default">
            <drawColor>#FFFFFFFF</drawColor>
            <fillColor>#FF000000</fillColor>
            <thickness>1</thickness>
        </template>
        <template name="solidred">
            <drawColor>WHITE</drawColor>
            <fillColor>RED</fillColor>
            <thickness>2</thickness>
        </template>
         <template name="solidblue">
            <drawColor>WHITE</drawColor>
            <fillColor>BLUE</fillColor>
            <thickness>2</thickness>
        </template>
    </templates>
</JMathAnimConfig>
