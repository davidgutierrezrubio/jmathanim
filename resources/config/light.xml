<JMathAnimConfig>
    <include>axes_and_functions_light.xml</include>
    <include>dots.xml</include>
    <background>
        <color>#FDFDFD</color>
    </background>
    <styles>
        <style name="default">
            <drawColor>black</drawColor>
            <fillColor>#00000000</fillColor>
            <thickness>1</thickness>
        </style>
        <style name="latexdefault">
            <drawColor>black</drawColor>
            <fillColor>black</fillColor>
            <thickness>.5</thickness>
        </style>
         <style name="FunctionGraphDefault">
            <drawColor>black</drawColor>
            <fillColor>TRANSPARENT</fillColor>
            <thickness>.5</thickness>
        </style>
        <style name="taylor">
            <drawColor>#fa867e</drawColor>
            <fillColor>TRANSPARENT</fillColor>
            <thickness>.5</thickness>
        </style>
        <style name="solidred">
            <drawColor>black</drawColor>
            <fillColor>red</fillColor>
            <thickness>2</thickness>
        </style>
        <style name="solidblue">
            <drawColor>black</drawColor>
            <fillColor>blue</fillColor>
            <thickness>2</thickness>
        </style>
    </styles>
</JMathAnimConfig>
