<JMathAnimConfig>
        <include>axes_and_functions_dark.xml</include>
        <include>dots.xml</include>
    <background>
        <color>#192841</color>
    </background>
    <styles>
        <style name="default">
            <drawColor>#FFFFFFFF</drawColor>
            <fillColor>#FF000000</fillColor>
            <thickness>1</thickness>
        </style>  
        <style name="latexdefault">
            <drawColor>#FFFFFFFF</drawColor>
            <fillColor>#FFFFFFFF</fillColor>
            <thickness>.05</thickness>
        </style>
        <style name="latextransparent">
            <drawColor>#FFFFFFFF</drawColor>
            <fillColor>#FFFFFF00</fillColor>
            <thickness>.04</thickness>
        </style>
        <style name="solidred">
            <drawColor>WHITE</drawColor>
            <fillColor>RED</fillColor>
            <thickness>1.5</thickness>
        </style>
        <style name="solidblue">
            <drawColor>WHITE</drawColor>
            <fillColor>BLUE</fillColor>
            <thickness>1.5</thickness>
        </style>
    </styles>
</JMathAnimConfig>
