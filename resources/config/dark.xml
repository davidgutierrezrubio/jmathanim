<JMathAnimConfig>
    <background>
        <color>#192841</color>
    </background>
    <styles>
        <style name="default">
            <drawColor>#FFFFFFFF</drawColor>
            <fillColor>#FF000000</fillColor>
            <thickness>1</thickness>
        </style>  
        <style name="latexdefault">
            <drawColor>#FFFFFFFF</drawColor>
            <fillColor>#FFFFFFFF</fillColor>
            <thickness>.05</thickness>
        </style>
        <style name="latextransparent">
            <drawColor>#FFFFFFFF</drawColor>
            <fillColor>#FFFFFF00</fillColor>
            <thickness>.04</thickness>
        </style>
        <style name="solidred">
            <drawColor>WHITE</drawColor>
            <fillColor>RED</fillColor>
            <thickness>1.5</thickness>
        </style>
        <style name="solidblue">
            <drawColor>WHITE</drawColor>
            <fillColor>BLUE</fillColor>
            <thickness>1.5</thickness>
        </style>
        <style name="axisblue1">
            <drawColor>#2565AE</drawColor>
            <fillColor>NONE</fillColor>
            <thickness>.5</thickness>
        </style>
        <style name="axisblue2">
            <drawColor>#6ca2e0</drawColor>
            <fillColor>NONE</fillColor>
            <thickness>.5</thickness>
        </style>
        <style name="dotRedCircle">
            <drawColor>RED</drawColor>
            <fillColor>NONE</fillColor>
            <thickness>.5</thickness>
            <dotStyle>DOT_STYLE_CIRCLE</dotStyle>
        </style>
        <style name="dotBlueCross">
            <drawColor>#6ca2e0</drawColor>
            <fillColor>NONE</fillColor>
            <thickness>.5</thickness>
            <dotStyle>cross</dotStyle>
        </style>
         <style name="dotYellowPlus">
            <drawColor>#FCE16D</drawColor>
            <fillColor>NONE</fillColor>
            <thickness>.5</thickness>
            <dotStyle>plus</dotStyle>
        </style>
    </styles>
</JMathAnimConfig>
